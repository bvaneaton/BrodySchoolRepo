package interpreter;

import program.*;


public class Interpreter {

	
	//gets the next instruction in the array of instruction
	public void readNextInstruction(){
		
		this.lastEatenInstruction = currentInstruction;
		
		
		
		switch(currentInstruction.getNoun()){
			//do something....
			case FORWARD:
				break;
			case CLOCKWISE:
				break;
			case COUNTERCLOCKWISE:
				break;
			default:
				break;
		
		}
		
	}
	
	
	
	//the current instruction being interpreted
	private Instruction currentInstruction;
	
	//the last instruction we just interpreted
	private Instruction lastEatenInstruction;
	
	private int xIndexOfInstructionList;
	
	private int yIndexOfInstructionList;
}
